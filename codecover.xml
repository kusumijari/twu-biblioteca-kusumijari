<SrcFile Content="//Contains book details in a certain format&#10;&#10;package com.tw.biblioteca;&#10;&#10;public class Book {&#10;    private String name, author;&#10;    private int yearOfPublication;&#10;&#10;    public Book(String name, String author, int yearOfPublication) {&#10;        this.name = name;&#10;        this.author = author;&#10;        this. yearOfPublication = yearOfPublication;&#10;    }&#10;&#10;    @Override&#10;    public String toString() {&#10;&#10;        return (name+&quot;\t&quot;+author+&quot;\t&quot;+yearOfPublication);&#10;    }&#10;&#10;}&#10;" Filename="Book.java" Intrnl_Id="7"/>
<SrcFile Content="//Parses the menu option entered by the user&#10;&#10;package com.tw.biblioteca;&#10;&#10;import java.util.ArrayList;&#10;&#10;public class Parser {&#10;    private String option;&#10;&#10;    public Parser(String option) {&#10;        this.option = option;&#10;    }&#10;&#10;    public boolean isValid() {&#10;        if(option.matches(&quot;[0-9]+&quot;) &amp;&amp; convertOptionToInt() &gt; 0 &amp;&amp; convertOptionToInt() &lt;= 2 ) {&#10;            return true;&#10;        }&#10;        return false;&#10;    }&#10;&#10;    private int convertOptionToInt() {&#10;        return Integer.parseInt(option);&#10;    }&#10;&#10;    public MenuItem createMenuItem() {&#10;        if(isValid()) {&#10;            if (convertOptionToInt() == 1) {&#10;                ArrayList&lt;Book&gt; list = new ArrayList&lt;Book&gt;();&#10;                Book book1 = new Book(&quot;Harry Potter&quot;, &quot;J. K. Rowling&quot;, 1997);&#10;                Book book2 = new Book(&quot;Tale of Two Cities&quot;, &quot;Charles Dickens&quot;, 1859);&#10;                list.add(book1);&#10;                list.add(book2);&#10;                return new BookList(list);&#10;            }&#10;            else if(convertOptionToInt() == 2) {&#10;                return new Exit();&#10;            }&#10;        }&#10;        return new InvalidMenuItem();&#10;    }&#10;}&#10;" Filename="Parser.java" Intrnl_Id="8"/>
<SrcFile Content="//contains a menu list that can be displayed&#10;&#10;package com.tw.biblioteca;&#10;&#10;import java.util.ArrayList;&#10;&#10;public class Menu {&#10;    ArrayList&lt;String&gt; menu= new ArrayList&lt;String&gt;();&#10;    Menu() {&#10;        menu.add(&quot;1. List Books&quot;);&#10;        menu.add(&quot;2. Exit&quot;);&#10;    }&#10;&#10;    @Override&#10;    public String toString() {&#10;        StringBuilder stringBuilder = new StringBuilder();&#10;        stringBuilder.append(&quot;Menu\n&quot;);&#10;        for(String menuItem : menu){&#10;            stringBuilder.append(menuItem+&quot;\n&quot;);&#10;        }&#10;        return stringBuilder.toString();&#10;    }&#10;}&#10;" Filename="Menu.java" Intrnl_Id="9"/>
